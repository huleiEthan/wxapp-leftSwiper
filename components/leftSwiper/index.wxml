<view class="left-swiper-class">
  <view class="item">
    <view bindtouchstart="touchS" bindtouchmove="touchM" bindtouchend="touchE" data-index="{{index}}" style="{{txtStyle}}" class="inner">
      <slot name="left-swiper-content"></slot>
    </view>
    <view class="swiper-area">
      <slot name="left-swiper-btn"></slot>
    </view>
  </view>
</view>
